  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jrichter1/vscode-extension-tester/issues"
  },
  "homepage": "https://github.com/jrichter1/vscode-extension-tester#readme",
  "devDependencies": {
    "@types/clipboardy": "^1.1.0",
    "@types/fs-extra": "^5.1.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.10",
    "@types/request": "^2.48.1",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "typedoc": "^0.15.0",
    "typescript": "^3.3.3333"
  },
